{
	"extends": [
		"plugin:@wordpress/eslint-plugin/recommended"
	],
	"rules": {
		"prettier/prettier": "off",
		"object-curly-spacing": [
			"error",
			"always"
		],
		"no-multi-spaces": "error",
		"space-in-parens": [
			"error",
			"always"
		],
		"array-bracket-spacing": [
			"error",
			"always"
		],
		"computed-property-spacing": [
			"error",
			"always"
		],
		"func-call-spacing": [
			"error",
			"never"
		]
	}
}
